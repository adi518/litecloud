env:
  browser: true
  es6: true
  node: true
extends: ['eslint:recommended', 'plugin:react/recommended']
parse: 'babel-eslint'
parserOptions:
  sourceType: module
  ecmaFeatures:
    experimentalObjectRestSpread: true
    jsx: true
plugins:
  - react
rules:
  no-console: 0
  indent:
    - error
  linebreak-style:
    - off
  quotes:
    - error
    - single
  semi:
    - off
